
#include<iostream>

using namespace std;

class cross{
    public:
        cross(int n);
        int fun_cross();
        void set(int);
        int num1;
        int num2;
    private:
        int a;
};

cross::cross(int n){
    void set(int n);
}

void cross::set(int n){
    a = n;
}

int cross::fun_cross(){
    for(int i=1;i<=a;i++){
        num1 = 1;
        num2 = 1;
        num1 = num1 * num2;
        num2++;    
    }
    return num1;
}

int main(){
    cross c(5);
    cout<<c.fun_cross()<<endl;
    
    system("pause");
    return 0;
}

